{
    "version": 2,
    "builds": [
      {
        
        "src": "public/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/award/(.*)",
        "dest": "/public/video.html"
      },
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  